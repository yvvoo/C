#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main()
{
    int number = 0;
    
    srand(time(NULL));
    
    for(int i=0; i<10; i++){
        number = rand();
        printf("number=%d\n", number);
    }
    return 0;
}


#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main()
{
    int number[10];
    
    srand(time(NULL));
    
    for (int i=0; i<10; i++){
        number[i]=(rand()%100)+1;  //1부터 100까지
    }
    
    for (int i=0; i<10; i++){
        printf("number=%d\n", number[i]);
    }
    
    
    return 0;
}

